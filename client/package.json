{
  "name": "client-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "docker:serve": "docker-compose up --build",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "jest",
    "cypress": "cypress run --browser chrome",
    "doc": "rm -r ../docs/client; typedoc . --options ./typedoc.js --theme markdown",
    "deploy": "scp -r dist/ ubuntu@ec2-18-224-235-29.us-east-2.compute.amazonaws.com:~",
    "simple": "npx ts-node --project tsconfig.json src/mock/simple-server.ts"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@types/pug": "^2.0.4",
    "colors": "^1.3.3",
    "core-js": "^2.6.5",
    "lodash": "^4.17.11",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "tiptap": "^1.22.1",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-mixin-decorator": "^1.1.1",
    "vue-p5": "^0.7.1",
    "vue-prism-editor": "^0.2.1",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-socket.io-extended": "^3.2.1",
    "vue-youtube": "^1.3.4",
    "vuetify": "^1.5.14",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/jest": "^23.1.4",
    "@types/lodash": "^4.14.134",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-cypress": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-polyfill": "^6.26.0",
    "jest": "^24.8.0",
    "prismjs": "^1.16.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "ts-loader": "^5.4.4",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.1",
    "typescript": "^3.4.5",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vue-typescript-jest": "^0.3.1",
    "vuetify-loader": "^1.0.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "vue": {
    "runtimeCompiler": true,
    "configureWebpack": {
      "resolve": {
        "alias": {
          "vue$": "vue/dist/vue.common.js"
        }
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(ts|vue)$": "<rootDir>/node_modules/vue-typescript-jest/preprocessor.js",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testRegex": "/tests/unit/.*\\.(ts|js)$",
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/.*\\.(ts|js)$",
      "/.*\\.vue$"
    ]
  }
}
